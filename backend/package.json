{
    "name": "nasa-project-backend",
    "version": "1.0.0",
    "description": "Express.js backend for NASA Explorer project",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "express": "^4.18.2",
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "eslint": "^8.40.0"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "keywords": [
        "nasa",
        "api",
        "express",
        "space",
        "astronomy"
    ],
    "author": "NASA Project Team",
    "license": "MIT",
    "jest": {
        "testEnvironment": "node",
        "collectCoverageFrom": [
            "**/*.js",
            "!node_modules/**",
            "!coverage/**"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "text",
            "lcov",
            "html"
        ]
    }
}